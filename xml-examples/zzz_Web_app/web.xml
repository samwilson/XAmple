<?xml version="1.0" encoding="UTF-8"?>
<!--DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN' 'http://java.sun.com/j2ee/dtds/web-app_2_2.dtd'-->
<web-app>
	<display-name>PHASE2</display-name>
	<distributable/>
	<servlet>
		<servlet-name>AudioServlet</servlet-name>
		<servlet-class>com.wmg.gar.asset.AudioServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>TestServlet</servlet-name>
		<servlet-class>com.wmg.gar.controller.TestServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>GARGroupAdminServlet</servlet-name>
		<servlet-class>com.wmg.gar.utils.GARGroupAdminServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>GARSearchLoginServlet</servlet-name>
		<servlet-class>com.wmg.gar.search.GARSearchLoginServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>GARSearchServlet</servlet-name>
		<servlet-class>com.wmg.gar.search.GARSearchServlet</servlet-class>
		<init-param>
			<param-name>TEMPLATE_FILE</param-name>
			<param-value>/u06/iplanet/ias6/ias/APPS/phase2/phase2/template.htm</param-value>
		</init-param>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>GARSearchAssetDetailServlet</servlet-name>
		<servlet-class>com.wmg.gar.search.GARSearchAssetDetailServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<resource-ref>
		<res-ref-name>jdbc/GarDataSource</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
	<login-config>
		<realm-name/>
		<form-login-config>
			<form-login-page/>
			<form-error-page/>
		</form-login-config>
	</login-config>
	<ejb-ref>
